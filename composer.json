{
    "name": "bluedaysolutions/faker-bundle",
    "description": "Put the awesome Faker lib into the DIC and populate your database with fake data. With Symfony 4+ compatibility",
    "keywords": ["faker", "fixtures", "data"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "William Durand",
            "email": "william.durand1@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "~4.0",
        "symfony/console": "4.4.*",
        "fakerphp/faker": "^1.21"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bazinga\\Bundle\\FakerBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
